.roundbutton__button {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  border-color: transparent;
}

.roundbutton__button--new {
  background-color: black;
}

.roundbutton__button--navigate {
  background-color: white;
  border: none;
}

.roundbutton__button--timer {
  background-color: purple;
}
